Algoritmo funciones_mayusculas_minusculas
	
	nombre = "Jose Delgado"

	// Convertir una cadena a Mayusculas mediante la función Mayusculas(cadena)
	Imprimir 'Nombre en mayusculas: ', Mayusculas(nombre)
	
	// Convertir una cadena a Minusculas mediante la función Minusculas(cadena)
	Imprimir 'Nombre en minusculas: ', Minusculas(nombre)
	
FinAlgoritmo